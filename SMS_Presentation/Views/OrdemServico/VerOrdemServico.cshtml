@using Presentation.Extensions
@model ERP_CRM_Solution.ViewModels.OrdemServicoViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Ordens de Serviço</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li>
                <a href="@Url.Action("MontarTelaOrdemServico", "OrdemServico")">Ordens de Serviço</a>
            </li>
            <li class="active">
                <strong>Ordens de Serviço - Consulta</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Ordens de Serviço - Consulta</h5>
                </div>
                <div class="ibox-content">
                    @Html.AntiForgeryToken()
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <row>
                        <div class="form-group">
                            @Html.ButtonLinkIndex(text: "Voltar", action: "MontarTelaOrdemServico", controller: "OrdemServico")
                            @Html.ButtonLinkIndex(text: "Dashboard", controller: "Servico", action: "MontarTelaDashboardServiceDesk")
                        </div>
                    </row>
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                            <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-10">Produto/Serviço</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-2">Anexos</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-3">Cancelamento</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-4">Acompanhamentos</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-5">Agendamento</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-6">Agendamentos - Lista</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-7">Observações</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-8">Atendimento de Origem</a></li>
                        </ul>
                        <div class="tab-content">
                            @*Dados Gerais*@
                            <div id="tab-1" class="tab-pane active">
                                <div class="panel-body">
                                    @using (Html.BeginForm("VerOrdemServico", "OrdemServico", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Status
                                                    @if (Model.ORSE_IN_STATUS == 999)
                                                    {
                                                        @Html.TextBox("status", "Criado", new { style = "color: orange; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 1)
                                                    {
                                                        @Html.TextBox("status", "Para Aprovação", new { style = "color: blue; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 3)
                                                    {
                                                        @Html.TextBox("status", "Finalizado", new { style = "color: green; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 4)
                                                    {
                                                        @Html.TextBox("status", "Cancelado ", new { style = "color: red; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 5)
                                                    {
                                                        @Html.TextBox("status", "Em Aprovação", new { style = "color: orange; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 6)
                                                    {
                                                        @Html.TextBox("status", "Aprovado", new { style = "color: green; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else if (Model.ORSE_IN_STATUS == 7)
                                                    {
                                                        @Html.TextBox("status", "Recusada", new { style = "color: red; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                    else
                                                    {
                                                        @Html.TextBox("status", "-", new { style = "color: blue; font-weight: bold;", @class = "form-control", @readonly = "readonly" })
                                                    }
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Categoria*
                                                    @Html.TextBoxFor(m => m.CATEGORIA_ORDEM_SERVICO.CAOS_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Responsável
                                                    @Html.TextBoxFor(m => m.USUARIO.USUA_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-xs-12">
                                                    Departamento
                                                    @Html.TextBoxFor(m => m.DEPARTAMENTO.DEPT_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                  <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Categoria*
                                                    @Html.TextBoxFor(m => m.CATEGORIA_ORDEM_SERVICO.CAOS_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Número*
                                                    @Html.TextBoxFor(m => m.ORSE_NR_NUMERO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                 <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Início
                                                    @Html.TextBoxFor(m => m.ORSE_DT_INICIO, new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:green; font-weight:bold" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data Prevista
                                                    @Html.TextBoxFor(m => m.ORSE_DT_PREVISTA, new { @class = "form-control", autofocus = true, @id = "dtPrevista", @readonly = "readonly", @style = "font-weight:bold" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Forma de Pagamento
                                                    @Html.TextBoxFor(m => m.FORMA_PAGAMENTO.FOPA_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                            <div class="form-group" >
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Filial
                                                    @Html.TextBoxFor(m => m.FILIAL.FILI_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Equipamento
                                                    @Html.TextBoxFor(m => m.ORSE_NM_EQUIPAMENTO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Número Equipamento
                                                    @Html.TextBoxFor(m => m.ORSE_NR_EQUIPAMENTO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Técnico
                                                    @Html.TextBoxFor(m => m.ORSE_IN_TECNICO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Valor Total (R$)
                                                    @Html.TextBox("vlrTotal", (decimal)ViewBag.ValorTotal, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Descrição*
                                                    @Html.TextAreaFor(m => m.ORSE_DS_DESCRICAO, new { @class = "form-control", style = "width: 100%; height: 100px;", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                            @if (Model.ORSE_IN_STATUS == 3)
                                            {
                                                <hr />
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Data/Hora de Encerramento
                                                        @Html.TextBoxFor(m => m.ORSE_DT_ENCERRAMENTO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                    </div>
                                                </div>
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    <div class="col-lg-12 col-md-12 col-xs-12">
                                                        Anotações de Encerramento
                                                        @Html.TextAreaFor(m => m.ORSE_DS_ENCERRAMENTO, new { @class = "form-control", style = "width: 100%; height: 100px;", autofocus = true, @readonly = "readonly" })
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    }
                                </div>
                            </div>

                            @*Imagens - Foto e Anexos*@
                            <div id="tab-2" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <hr />
                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                            <h4>Anexos da Ordem de Serviço</h4>
                                            <br />
                                            <div class="form-group">
                                                @using (Html.BeginForm("UploadFileOrdemServico", "OrdemServico", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data", @id = "pwd-container2" }))
                                                {
                                                    <div class="ibox-content">
                                                        @if (Model.ORDEM_SERVICO_ANEXO != null && Model.ORDEM_SERVICO_ANEXO.Count > 0)
                                                        {
                                                            <table class="table table-striped table-bordered table-hover dataTables-example">
                                                                <thead style="background-color:lightsteelblue">
                                                                    <tr>
                                                                        <th>Título</th>
                                                                        <th>Data</th>
                                                                        <th>Tipo</th>
                                                                        <th class="td-one-action">Ações</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @foreach (var cond in (Model.ORDEM_SERVICO_ANEXO))
                                                                    {
                                                                        <tr>
                                                                            <td>@cond.ORSX_NM_TITULO</td>
                                                                            <td>@cond.ORSX_DT_ANEXO.ToShortDateString()</td>
                                                                            @if (cond.ORSX_IN_TIPO == 1)
                                                                            {
                                                                                <td>Imagem</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Arquivo</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @if (cond.ORSX_IN_TIPO == 1 || cond.ORSX_IN_TIPO == 2)
                                                                                {
                                                                                    @Html.TableLinkDetails(cond.ORSX_CD_ID, "VerAnexoOrdemServico", "fa-camera", "Visualizar")
                                                                                }
                                                                                @if (cond.ORSX_IN_TIPO == 3)
                                                                                {
                                                                                    <a title="Visualizar" class="fa fa-camera fa-lg" href="~/PdfViewer/web/viewer.html?file=@(cond.ORSX_AQ_ARQUIVO.Replace("~", ""))&ReturnUrl=@Request.Url.AbsolutePath" target="_blank"></a>
                                                                                }
                                                                                @Html.TableLinkDetails(cond.ORSX_CD_ID, "DownloadOrdemServico", "fa-download", "Download")
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        }
                                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                                            <input style="float:left; margin-right:5px;" type="file" name="file" id="file" class="btn btn-sm btn-warning" />
                                                            <input style="float:left;" type="submit" value="Incluir Anexo" class="btn btn-sm btn-success" />
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Cancelamento*@
                            <div id="tab-3" class="tab-pane">
                                <div class="panel-body">
                                    @using (Html.BeginForm("EditarOrdemServico", "OrdemServico", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Data/Hora de Cancelamento
                                                    @Html.TextBoxFor(m => m.ORSE_DT_CANCELAMENTO, new { @class = "form-control", autofocus = true, @readonly = "readonly", @style = "color:red; font-weight:bold" })
                                                </div>
                                                @*<div class="col-lg-2 col-md-2 col-xs-12">
                                                        Hora Cancelamento
                                                        @Html.TextBoxFor(m => m.ATEN_HR_CANCELAMENTO, new { @class = "form-control", autofocus = true, @readonly="readonly" })
                                                    </div>*@
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Anotações de Cancelamento
                                                    @Html.TextAreaFor(m => m.ORSE_DS_MOTIVO_CANCELAMENTO, new { @class = "form-control", style = "width: 100%; height: 100px;", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                    }

                                </div>
                            </div>

                            @*Acompanhamentos*@
                            <div id="tab-4" class="tab-pane">
                                <div class="panel-body">
                                    <hr />
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <h4>Acompanhamentos:</h4>
                                            @if (Model.ORDEM_SERVICO_ACOMPANHAMENTO != null && Model.ORDEM_SERVICO_ACOMPANHAMENTO.Count > 0)
                                            {
                                                foreach (var item in (Model.ORDEM_SERVICO_ACOMPANHAMENTO).OrderByDescending(a => a.ORSA_DT_ACOMPANHAMENTO))
                                                {
                                                    <div class="social-feed-box">
                                                        <div class="social-avatar">
                                                            <a href="" class="pull-left">
                                                                <img src="@Url.Content(item.USUARIO.USUA_AQ_FOTO)" style="width: auto; height: auto; max-height: 94px; max-width: 90px;">
                                                            </a>
                                                            <div class="media-body">
                                                                <a href="#">
                                                                    @item.USUARIO.USUA_NM_NOME
                                                                </a>
                                                                @if (item.ORSA_DT_ACOMPANHAMENTO != null && item.ORSA_DT_ACOMPANHAMENTO != null)
                                                                { 
                                                                    <small class="text-muted">@item.ORSA_DT_ACOMPANHAMENTO.Value.ToLongDateString() as @item.ORSA_DT_ACOMPANHAMENTO.Value.ToShortTimeString()</small>
                                                                }
                                                            </div>
                                                        </div>
                                                        <div class="social-body">
                                                            <p>
                                                                @item.ORSA_DS_DESCRICAO
                                                            </p>
                                                        </div>
                                                    </div>
                                                }
                                            }
                                        </div>
                                    </div>
                                    <br />
                                    <div class="form-group">
                                        @Html.ButtonLinkIndex(text: "Incluir Acompanhamento", action: "IncluirAcompanhamento", controller: "OrdemServico")
                                        @Html.ButtonLinkIndex(text: "Incluir Acompanhamento e Encerrar", action: "IncluirAcompanhamentoEncerrar", controller: "OrdemServico")
                                    </div>
                                </div>
                            </div>

                            @*Agendamento*@
                            <div id="tab-5" class="tab-pane">
                                <div class="panel-body">
                                    @using (Html.BeginForm("IncluirAgendamento", "OrdemServico", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container2" }))
                                    {
                                        <div class="row">
                                            <div class="form-group">
                                                <div class="col-sm-3 col-md-3 col-xs-12">
                                                    @Html.HiddenFor(m => m.ORSE_CD_ID)
                                                    Categoria
                                                    @Html.DropDownList("CAAG_CD_ID", (SelectList)ViewBag.CatsAgenda, "Selecionar", new { @class = "form-control", @data_val = "true" })
                                                </div>
                                                <div class="col-sm-3 col-md-3 col-xs-12">
                                                    Data
                                                    @Html.TextBox(name: "AGEN_DT_DATA", value: DateTime.Now.Date.ToString("dd//MM/yyyy"), format: "{0:dd/MM/yyyy}", htmlAttributes: new { @class = "form-control date-picker", autofocus = true })
                                                </div>
                                                <div class="col-sm-3 col-md-3 col-xs-12">
                                                    Hora
                                                    <div class="input-group date" id="datetimepicker3">
                                                        @Html.TextBox(name: "AGEN_HR_HORA", value: DateTime.Now.TimeOfDay, htmlAttributes: new { @class = "form-control time-picker", autofocus = true, @id = "hora" })
                                                        <span class="input-group-addon">
                                                            <span class="glyphicon glyphicon-time"></span>
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3 col-md-3 col-xs-12">
                                                    Contato
                                                    @Html.DropDownList("AGEN_CD_USUARIO", (SelectList)ViewBag.Usuarios, "Selecionar", new { @class = "form-control", @id = "AGEN_CD_USUARIO", @data_val = "true" })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="col-sm-12 col-md-12 col-xs-12">
                                                    Título
                                                    @Html.TextBox(name: "AGEN_NM_TITULO", value: null, htmlAttributes: new { @class = "form-control", autofocus = true })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="col-sm-12 col-md-12 col-xs-12">
                                                    Descrição
                                                    @Html.TextArea(name: "AGEN_DS_DESCRICAO", value: null, htmlAttributes: new { @class = "form-control", autofocus = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            @Html.ButtonActionSubmit(text: "Incluir Agendamento", htmlAttributes: new { id = "basictoast" })
                                        </div>
                                    }
                                </div>
                            </div>

                            @*Agendamentos - Lista*@
                            <div id="tab-6" class="tab-pane">
                                <div class="panel-body">
                                    <h5>Agendamentos</h5>
                                    <br />
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-sm-12 col-md-12 col-xs-12">
                                                <table class="table table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th>Data</th>
                                                            <th>Hora</th>
                                                            <th>Título</th>
                                                            <th>Categoria</th>
                                                            <th>Ações</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @if (ViewBag.Agendamentos != null && ViewBag.Agendamentos.Count > 0)
                                                        {
                                                            foreach (var cond in (List<EntitiesServices.Model.AGENDA>)ViewBag.Agendamentos)
                                                            {
                                                                <tr>
                                                                    <td>@cond.AGEN_DT_DATA.ToShortDateString()</td>
                                                                    <td>@cond.AGEN_HR_HORA.ToString()</td>
                                                                    <td>@cond.AGEN_NM_TITULO</td>
                                                                    <td>@cond.CATEGORIA_AGENDA.CAAG_NM_NOME</td>
                                                                    <td>
                                                                        @Html.TableLinkDetails(cond.AGEN_CD_ID, "EditarAgenda", "fa-edit", "Editar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        }
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Observações*@
                            <div id="tab-7" class="tab-pane">
                                <div class="panel-body">
                                    @using (Html.BeginForm("EditarOrdemServico", "OrdemServico", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Observações
                                                    @Html.TextAreaFor(m => m.ORSE_TX_OBSERVACOES, new { @class = "form-control summernote", style = "width: 100%; height: 400px;", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>

                             @*Atendimento Origem*@
                            <div id="tab-8" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                            <div class="form-group" >
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    Atendimento
                                                    @Html.TextBoxFor(m => m.ATENDIMENTO.ATEN_NM_ASSUNTO, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                </div>
                                            </div>
                                        </div>
                                </div>
                            </div>

                            
                             @*Produto/Servico*@
                            <div id="tab-10" class="tab-pane">
                                <div class="panel-body">
                                     <div class="row">
                                         <h3>Produtos</h3>
                                         <div class="form-group">
                                             <div class="col-sm-12 col-md-12 col-xs-12">
                                                <table class="table table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th>Produto</th>
                                                            <th>Preço (R$)</th>
                                                            <th>Preço Promoção (R$)</th>
                                                            <th>Quantidade</th>
                                                            <th>Preço Total (R$)</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var cond in Model.ORDEM_SERVICO_PRODUTO)
                                                        {
                                                                <tr>
                                                                    <td>@cond.PRODUTO.PROD_NM_NOME</td>
                                                                    <td>@cond.OSPR_VL_PRECO_NOVO</td>
                                                                    <td>@cond.OSPR_VL_PRECO_PROMO</td>
                                                                    <td>@cond.OSPR_IN_QUANTIDADE</td>
                                                                    <td>@CrossCutting.Formatters.DecimalFormatter((Int32)cond.OSPR_IN_QUANTIDADE * (Decimal)cond.OSPR_VL_PRECO_NOVO)</td>
                                                                </tr>
                                                            }
                                                    </tbody>
                                                </table>
                                            </div>
                                         </div>
                                     </div>

                                     <div class="row">
