    <script>
        function DesativaServico(val) {
            if (val != "") {
                $.ajax({
                    url: '../GetCustoProduto'
                    , type: 'POST'
                    , data: {
                        id: val
                    }
                    , success: function (r) {
                        $('#preco').val("");
                        if (r.preco != null) {
                            $('#preco').val(r.preco);
                        }
                        if (r.promo != null) {
                            $('#promo').val(r.promo);
                        }
                    }
                    , error: function () {
                        $('#preco').val("");
                        $('#promo').val("");
                    }
                });
            }
        }

        function DesativaProduto(val) {
            if (val != "") {
                $.ajax({
                    url: '../GetCustoServico'
