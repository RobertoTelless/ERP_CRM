         function IncluirProd() {
             var prod = $('#lstProduto').val();
             var qtde = $('#qtde').val();
             var preco = $('#preco').val();
             var promo = $('#promo').val();
             var desc = $('#desc').val();

             var data = {
                 PROD_CD_ID: prod
                 , qtde: qtde
                 , preco: preco
                 , item_obs: desc
                 , promo: promo
             }
             $.ajax({
                 url: '../IncluirProdutoOS_Inline'
                 , type: 'POST'
                 , data: data
                 , success: function (r) {
                     if (prod != "") {
                         $('table#tblProd > tbody').append('<tr>'
                             + '<td>' + $('#lstProduto option:selected').text() + '</td>'
                             + '<td>' + preco + '</td>'
                             + '<td>' + promo + '</td>'
                             + '<td>' + qtde + '</td>'
                             + '<td>' + parseFloat(qtde * preco.replace(',', '.')).toFixed(2) + '</td>'
                             + '<td><a alt="Excluir" class="tbl-link fa-lg fa fa-trash-o" href="/OrdemServico/ExcluirOSProduto/' + r.id + '" target="_self" title="Excluir"> </a></td></tr>'
                         );
                     }

                     $('#vlrTotalProd').html('<strong>Valor Produtos: </strong> R$' + r.vlrTotal);

                     $('.select2-selection__clear').click();
                     $('#qtde').val("");
                     $('#preco').val("");
                     $('#promo').val("");
                     $('#desc').val("");
                 }
             });
         }

